<div class="nested-fields">
  <div class="row">
    <div class="col-sm-2">
      <%= link_to_remove_association "remove team", f %>
    </div>
    <div class="col-sm-5">
      <p><%= f.input :athleteteam, label: "Team Name:"%></p>
    </div>
    <div class="col-sm-5">
      <p><%= f.input :website, label: "Team Website:"%></p>
    </div>
  </div>

  <div class="row">
    <div class="col-sm-2">
    </div>

    <div class="col-sm-5">

      <p><%= f.input :startyear, collection: [ '2014','2013','2012','2011','2010','2009','2008','2007','2006','2005','2004','2003','2002','2001','2000','1999','1998','1997','1996','1995','1994','1993','1992','1991','1990'],:prompt => "Select Start Date",label: "Start:" , class: 'input-sm'%></p>
    </div>

    <div class="col-sm-5">
      <p><%= f.input :endyear, collection: [ '2014','2013','2012','2011','2010','2009','2008','2007','2006','2005','2004','2003','2002','2001','2000','1999','1998','1997','1996','1995','1994','1993','1992','1991','1990','Current'],:prompt => "Select End Date",label: "End:" %></p>
    </div>

  </div>

  <p><%= f.file_field :teamlogo %></p>
  <p><%= image_tag f.object.teamlogo.url(:thumb) unless f.object.teamlogo.url(:thumb).match('missing.png') %></p>
</div><hr>
